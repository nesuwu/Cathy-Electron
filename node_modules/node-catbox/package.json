{
	"name": "node-catbox",
	"version": "3.0.1",
	"author": "depthbomb",
	"description": "A library for interacting with Catbox.moe written in TypeScript.",
	"license": "MIT",
	"engines": {
		"node": ">=19.0.0"
	},
	"keywords": [
		"catbox.moe",
		"catbox",
		"typescript",
		"fetch",
		"api"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/depthbomb/node-catbox.git"
	},
	"bugs": {
		"url": "https://github.com/depthbomb/node-catbox/issues"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"start": "node .",
		"build": "tsc",
		"dist": "tsup",
		"release": "yarn dist && release-it",
		"lint": "eslint ./src --ext .ts"
	},
	"devDependencies": {
		"@types/eslint": "^8.44.4",
		"@types/node": "^20.8.4",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"@typescript-eslint/parser": "^6.7.5",
		"eslint": "^8.51.0",
		"release-it": "^16.2.1",
		"terser": "^5.21.0",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2"
	},
	"packageManager": "yarn@4.0.0-rc.53"
}
